@model MyElearningProject.DAL.Entities.Category
@{
    ViewBag.Title = "AddCategory";
    Layout = "~/Views/AdminLayout/AdminIndex.cshtml";
}

<h2>Yeni Kategori Ekleme Sayfası</h2>
<form method="post">
    <label>Kategori Adını Giriniz</label> <br />
    @Html.TextBoxFor(x=>x.CategoryName, new {@class="form-control"})<br />
    <br />
    <button class="btn btn-primary">Ekle</button>
</form>

